{
  "name": "Credits Testnet wallet 4.1",
  "description": "Wallet plugin for Credits testnet 4.1",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "128": "img/logo128.png",
    "64": "img/logo.png"
  },
  "browser_action": {
    "default_popup": "src/popup.html"
  },
  "version": "1.0.2",
  "short_name": "credits-wallet",
  "permissions": [
    "storage",
	  "tabs",
    "activeTab"
  ],
   "description": "This extension makes it possible to use a wallet on the Credits testnet",
   "author": "Timo Koen"
}
